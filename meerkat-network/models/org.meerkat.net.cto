namespace org.meerkat.net

asset Invoice identified by invoiceId {
  o String invoiceId
  o Double amount
  o InvoiceStatus status
  --> BizEntity sender
  --> BizEntity receiver
}

participant BizEntity identified by bizEntityId {
  o String bizEntityId
  o String name
  o Double debt
  o Double claim
}

enum InvoiceStatus {
  o NEW
  o ACCEPTED
  o REJECTED
  o PAID
  o COMPLETED
}
